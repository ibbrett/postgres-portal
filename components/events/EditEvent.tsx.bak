import {FaEdit} from 'react-icons/fa'
import Link from 'next/link'

type EditProps = {
  id: number
  section_id: number
}

export function EditEvent({id, section_id}: EditProps) {
  console.log('id, section_id', id, section_id)
  return (
    <Link
      href={{
        pathname: '/sections/event/edit',
        query: {
          id: id,
          section_id: section_id,
        },
      }}
    >
      <FaEdit />
    </Link>
  )
}
